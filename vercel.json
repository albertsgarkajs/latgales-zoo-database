{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    },
    {
      "src": "animals.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "server.js" },
    { "src": "/logout", "dest": "server.js" },
    { "src": "/style.css", "dest": "/style.css" },
    { "src": "/animals.json", "dest": "/animals.json" },
    { "src": "/login.html", "dest": "/login.html" },
    { "src": "/register.html", "dest": "/register.html" },
    { "src": "/dashboard.html", "dest": "/dashboard.html" },
    { "src": "/admin.html", "dest": "/admin.html" },
    { "src": "/index.html", "dest": "/index.html" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
