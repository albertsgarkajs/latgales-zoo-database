{
  "version": 2,
  "builds": [
    {
      "src": "api/auth.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth.js"
    },
    {
      "src": "/index-pc.html",
      "dest": "/index-pc.html"
    },
    {
      "src": "/index-mobile.html",
      "dest": "/index-mobile.html"
    },
    {
      "src": "/visitorcount.html",
      "dest": "/visitorcount.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
